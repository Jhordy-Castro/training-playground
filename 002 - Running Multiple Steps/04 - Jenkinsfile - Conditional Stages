pipeline {
    agent { 
		docker { 
			image 'alpine:latest' 
		}
	}
	
    parameters{
    	//string(name : "NAME", defaultValue : 'Mr.Jenkins', description : 'Whats your name')
	//string(name : "AGE", defaultValue : '0', description : 'Whats your age')
	string(name : "TOEXECUTE", defaultValue : 'defaultExecute', description : 'ToExecute?')
    }
	
    stages {
    	stage('build'){
		when {
			environment name : 'TOEXECUTE', value: 'build'
		}
	}
	
	stage('test'){
		when {
			environment name : 'TOEXECUTE', value: 'test'
		}
	}
	
    }
}
